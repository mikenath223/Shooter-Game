{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "arrow-parens": "off",
      "guard-for-in": "off",
      "max-classes-per-file": "off",
      "no-restricted-syntax": "off",
      "no-use-before-define": "off",
      "class-methods-use-this": "off"
    }
  }